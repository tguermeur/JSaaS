{
  "name": "js-saas-v2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3006 --force",
    "start": "react-scripts start",
    "build": "node --max-old-space-size=8192 ./node_modules/.bin/vite build",
    "build:extension": "npm run generate-icon && node scripts/download-firebase.js && vite build --config vite.config.extension.js && node scripts/build-extension.js",
    "build:extension-zip": "node scripts/build-extension-zip.js",
    "build:extension-folder": "node scripts/build-extension-folder.js",
    "build:extension-all": "npm run build:extension-zip && npm run build:extension-folder",
    "fix:extension": "node scripts/fix-extension.js",
    "watch:extension": "node scripts/watch-extension.js",
    "generate-icon": "node scripts/generate-icon.js",
    "preview": "vite preview",
    "clean": "rm -rf node_modules/.vite node_modules/.cache && npm install",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prebuild": "node scripts/copy-extension.js && cd public/extension && zip -r extension.zip *",
    "deploy": "npm run build && firebase deploy",
    "deploy:hosting": "npm run build && firebase deploy --only hosting",
    "deploy:functions": "firebase deploy --only functions",
    "deploy:firestore": "firebase deploy --only firestore",
    "deploy:storage": "firebase deploy --only storage",
    "ios:setup": "npx react-native-cli init JSaaSiOS --template react-native-template-typescript",
    "ios:run": "cd JSaaSiOS && npx react-native run-ios",
    "ios:build": "cd JSaaSiOS && npx react-native run-ios --configuration Release",
    "capacitor:init": "npx @capacitor/cli init",
    "capacitor:add-ios": "npx @capacitor/cli add ios",
    "capacitor:build": "npm run build && npx @capacitor/cli copy",
    "capacitor:sync": "npx @capacitor/cli sync",
    "capacitor:open-ios": "npx @capacitor/cli open ios"
  },
  "dependencies": {
    "@adobe/pdfservices-node-sdk": "^4.1.0",
    "@emailjs/browser": "^4.4.1",
    "@emotion/react": "11.11.1",
    "@emotion/styled": "11.11.0",
    "@firebase/firestore": "^4.7.10",
    "@firebase/firestore-compat": "^0.3.45",
    "@fullcalendar/core": "^6.1.17",
    "@fullcalendar/daygrid": "^6.1.17",
    "@fullcalendar/interaction": "^6.1.17",
    "@fullcalendar/react": "^6.1.17",
    "@fullcalendar/timegrid": "^6.1.17",
    "@mui/icons-material": "^5.14.0",
    "@mui/material": "5.14.0",
    "@mui/x-date-pickers": "5.0.20",
    "@react-pdf/renderer": "^4.3.0",
    "@stripe/stripe-js": "^7.1.0",
    "@types/jszip": "^3.4.0",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/uuid": "^10.0.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "buffer": "^6.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "2.30.0",
    "docx-preview": "^0.3.6",
    "docxtemplater": "^3.66.3",
    "esbuild": "^0.25.1",
    "firebase": "^11.5.0",
    "html2canvas": "^1.4.1",
    "html2pdf.js": "^0.10.3",
    "jspdf": "^3.0.1",
    "jszip": "^3.10.1",
    "lucide-react": "^0.503.0",
    "mammoth": "^1.10.0",
    "notistack": "^3.0.2",
    "nth-check": "^2.1.1",
    "papaparse": "^5.5.2",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^4.8.69",
    "pizzip": "^3.2.0",
    "postcss": "^8.5.3",
    "pptxtemplater": "^1.0.5",
    "react": "18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "18.2.0",
    "react-draggable": "^4.4.6",
    "react-dropzone": "^14.3.8",
    "react-pdf": "^9.2.1",
    "react-router-dom": "6.14.1",
    "react-to-pdf": "^2.0.1",
    "slate": "^0.117.2",
    "slate-react": "^0.117.2",
    "tailwind-merge": "^3.2.0",
    "uuid": "^11.1.0",
    "vite": "^6.2.3"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.0.0",
    "@capacitor/core": "^5.0.0",
    "@capacitor/ios": "^5.0.0",
    "@types/papaparse": "^5.3.15",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.61.0",
    "@typescript-eslint/parser": "5.61.0",
    "@vitejs/plugin-react": "^4.3.4",
    "chokidar": "^4.0.3",
    "fs-extra": "^11.3.0",
    "sharp": "^0.33.5",
    "terser": "^5.43.1",
    "typescript": "^4.9.5"
  },
  "overrides": {
    "@firebase/app": "^0.14.1"
  },
  "resolutions": {
    "@firebase/app": "^0.14.1"
  }
}
